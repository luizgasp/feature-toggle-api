generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model Features {
  id         String   @id @default(uuid())
  enabled    Boolean
  created_at DateTime @default(now())
  updated_at DateTime @updatedAt
  name       String   @unique

  @@map("features")
}
